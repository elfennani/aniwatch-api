{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "serverless.ts": {
      "includeFiles": "dist/**",
      "runtime": "@vercel/node@3.2.7"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/serverless.js"
    }
  ]
}
